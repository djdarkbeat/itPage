shed loading					thumb.css({display:'none',opacity:_fadeTo}).fadeIn(1500);										// hover effects					thumb.hover(						function() { thumb.fadeTo('fast',1); },						function() { _li.not('.active').children('img').fadeTo('fast',0.3); } // don't fade out if the parent is active					)				}			});		});		// END Galleria-specific JS	</script>	<style media="screen,projection" type="text/css">		/* BEGIN GALLERY-SPECIFIC STYLE - DO NOT CHANGE */		body{padding:20px;text-align:center;background:black;color:#fff;}    </style>[/If]</head><!-- END page_header -->