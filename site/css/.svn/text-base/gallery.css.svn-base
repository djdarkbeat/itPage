/* ---------------------------------------------- */
/* Standard Tags - Used throughout - OK to change */
/* ---------------------------------------------- */
body { background: #000;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 140%;
	margin: 30px 30px 0 30px; }
h1	{ font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 20px;
	margin-bottom: 5px; }
h2	{ font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 17px; }
h3	{ font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	line-height: 14pt;
	border-bottom: 1px dotted silver;
	}
h4	{ font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 13px; }
hr	{ color: #CCC;
	height: 2px;
	line-height: 100%;
	width: 95%; }
p {	font-size: 11px;
	color: #FFF;
	line-height: 150%;
	text-align: left; }
b	{ font-weight: bold; }
img { border-style: none; }
table { border-style: none; }
td 	{ font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #FFF; }
u	{ text-decoration: underline; }

ul	{font-size: 11px;
	color: #FFF;
	line-height: 170%;
	text-align: left; 
	margin: 10px 0 0 30px; }

/* Standard Links */
a:link, a:visited, a:hover { text-decoration: underline;
	color: #FFFF99; }

/* ---------------------------------------- */
/* Classes - Used throughout - OK to change */
/* ---------------------------------------- */
.border { background: #000;
	border-left: 1px solid #FFF; }
.containeryellow { background-color: #FF9;
	padding: 10px 10px 10px 20px; }
/* .contentpanel { font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #FFF;
	background-color: #FFF; } */
/* -------------------------------------- */
/* Forms - OK to Change                   */
/* -------------------------------------- */
form { 
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	margin-top: 0;
	margin-bottom: 0;
	background: #e9ffaa;
	color: #FFF; }
checkbox, input, radio, select, textarea {
	font-family: Verdana, Arial, sans-serif;
	font-size: 11px;
	vertical-align: bottom;
	margin-top: 3px;
	margin-bottom: 5px;
	margin-left: 3px;
	background: #FFF;
	color: #FFF; }
.formlabel	{
	background: #DDD;
	margin: 0;
	padding: 5px 10px 5px;
	color: #FFF;
	line-height: 1.3;
	font-size: 11px;
	vertical-align: bottom;}
.formlabelbold	{
	margin: 0;
	padding: 5px 10px 5px;
	color: #FFF;
	line-height: 1.3;
	font-size: 11px;
	font-weight: bold;
	vertical-align: bottom; }

/* -------------------------------------- */
/* Left Col - OK to change                */
/* -------------------------------------- */
.LeftCol { background: #FFF;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	line-height: 120%;
	color: #FFF; }
.leftnavcontainer {
	width: 161px;
	padding: 0 0 5px 0; }
hr.leftnav {
	color: #fcff91;
	height: 2px;
	width: 95%; }
/* Left column links */
a.LeftColLink:link, a.LeftColLink:visited {
	color: #300;
	text-align: right;
	text-indent: 10px;
	text-decoration: none;	
	background-color: transparent; }
a.LeftColLink:hover {
	color: #300;
	text-align: right;
	text-indent: 10px;
	text-decoration: none; }

/* -------------------------------------- */
/* Right Col - OK to change               */
/* -------------------------------------- */
.RightCol {	background: #DCDCDC;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	line-height: 120%;
	color: #FFF; }
/* Right column links */
a.RightColLink:link, a.RightColLink:visited {
	text-align: left;
	text-indent: 11px;
	text-decoration: none;	
	background-color: transparent;
	font-weight: bold;
	color: #FFF; }
a.RightColLink:hover	{	
	text-align: left;
	text-indent: 11px;
	text-decoration: none;	
	background-color: transparent;
	font-weight: bold;
	color: #FFF; }

/* -------------------------------------- */
/* Main Content Panel - OK to change      */
/* -------------------------------------- */
/* Content Panel */
.ContentPanel {
	font-size: 11px;
	text-align: left;
	background-color: #333;
	font-weight: normal;
	color: #FFF;
	padding: 10px 30px 5px 20px; }
.ContentPanelHead {
	font-size: 16px;
	text-align: left;
	font-weight: bold;
	color: yellow;
/*	width: 700px; */
	padding: 0 5px 5px 0; }
.ContentPanelSubhead {
	font-size: 12px;
	text-align: left;
	font-weight: bold;
	color: #FFF;
/*	width: 700px; */
	padding: 0 5px 5px 0; }
.ContentPanelAuthor {
	font-size: 11px;
	text-align: left;
	color: #FFF;
	padding: 5px 5px 2px 130px;
	font-style: italic; }
.ContentPanelCaption {
	font: italic 13px Georgia, "Times New Roman", Times, serif;
	text-align: left;
	color: #FFF;
	padding: 5px 5px 15px 0; }
.ContentPanelDate {
	font-size: 10px;
	text-align: left;
	font-weight: normal;
	color: gray;
/*	width: 700px; */
	padding: 0 5px 5px 0; }
.ContentPanelImage {
	padding: 0 30px 5px 0;
	vertical-align: top;
}

/* Content Panel Links */
a.ContentPanel:link, a.ContentPanel:visited {
	font-size: 11px;
	text-align: center;
	text-decoration: underline;
	background-color: transparent;
	font-weight: normal;
	color: #FFF;
	padding: 0 5px 5px 10px; }
a.ContentPanel:hover {
	font-size: 11px;
	text-align: center;
	text-decoration: underline;
	background-color: transparent;
	font-weight: normal;
	color: #FFF; }
/* -------------------------------------- */
/* Portfolios - OK to change              */
