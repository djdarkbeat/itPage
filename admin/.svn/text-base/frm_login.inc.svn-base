<?Lassoscript// Last modified 7/24/08 by ECL, Landmann InterActive// FUNCTIONALITY// Login form// CHANGE NOTES// 10/12/07// Recoded for CMS v. 3.0// 12/20/07// Added hidden form field for Action=Login// Changed form action from loginresponse.lasso to login.lasso// Moved Session_AddVariable for $svUser_ID, $svUserPrivs_Priv, $svAdmin_FName and $svAdmin_LName to login.lasso// 5/12/08// Ported changes from LBT// 6/11/08// Added autocomplete="off" to form statement// 7/24/08// Changed form action to be Response_FIlepath// NOTE: DO NOT change the name of this form, as it will break the at-end handler.If: $svDebug == 'Y';	'<p class="debug"><strong>/admin/frm_login</strong></p>\n';/If;?><div align="left"><h2>Please Log In</h2><form action="[Response_Filepath]" method="post" autocomplete="off" name="LoginForm">    <table>		<tr>			<td width="80" class="tablehead_10_white_right">Login ID</td>			<td width="230" bgcolor="#f0f0f0">				<input type="text" name="User_LoginID" value="[Var:'vUser_LoginID']" maxlength="64" id="User_LoginID"> 			</td>			<td></td>		</tr>		<tr>			<td width="80" class="tablehead_10_white_right">Password</td>			<td width="230" bgcolor="#f0f0f0">				<input type="password" name="User_LoginPW" value="[Var:'vUser_LoginPWClear']" maxlength="24" id="User_LoginPW"> 			</td>			<td></td>		</tr>		<tr>			<td colspan="2" bgcolor="#666666">				<div align="center">					<input type="submit" name="-Nothing" value="Login">					<input type="hidden" name="Action" value="Login">				</div>			</td>			<td></td>		  </tr>		<tr>			<td colspan="2">				<div align="center"><a href="/reset.lasso" target="_blank">Reset My Password</a></div>			</td>			<td></td>		</tr>    </table></form></div>